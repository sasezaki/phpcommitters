<?xml version="1.0"?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package>

  <name>phpcommitters</name>
  <summary>show php committers</summary>
  <description>
Prints the list of people who've committed to the PHP project
  </description>

  <license>PHP</license>

  <maintainers>
    <maintainer>
      <user>sasezaki</user>
      <name>Kazusuke Sasezaki</name>
      <email>sasezaki@gmail.com</email>
      <role>lead</role>
    </maintainer>
  </maintainers>

  <release>
    <version>1.0.0</version>
    <state>stable</state>
    <notes>- Initial release.
</notes>
    <date>2010-01-07</date>
  </release>
  <changelog>

  </changelog>
  <deps>
    <dep type="php" rel="ge" version="4.0.0"/>
  </deps>

  <filelist>
    <dir name="/">
      <file role='doc' name='CREDITS'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='phpcommitters.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='phpcommitters.c'/>
      <file role='src' name='php_phpcommitters.h'/>
      <dir name="tests">
        <file role='test' name='phpcommitters.phpt'/>
      </dir>
    </dir>
  </filelist>
</package>
